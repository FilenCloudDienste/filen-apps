    "expo-router": "~6.0.14",
    "expo-screen-orientation": "~9.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-task-manager": "~14.0.8",
    "expo-video": "~3.0.14",
    "expo-video-thumbnails": "~10.0.7",
    "expo-web-browser": "~15.0.9",
    "msgpackr": "^1.11.5",
    "react": "19.1.0",
