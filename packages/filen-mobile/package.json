    "expo-image-picker": "~17.0.8",
    "expo-intent-launcher": "~13.0.7",
    "expo-keep-awake": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-localization": "~17.0.7",
    "expo-media-library": "~18.2.0",
    "expo-navigation-bar": "~5.0.9",
