    "@types/uuid": "^10.0.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "npm-check-updates": "^19.1.2",
