{
	"name": "filen-mobile",
	"main": "./src/entry",
	"version": "0.0.0",
	"private": false,
	"scripts": {
		"start": "expo start --clear",
		"android": "expo run:android",
		"android:device": "expo run:android --device",
		"android:release": "expo run:android --variant release",
		"android:release:device": "expo run:android --variant release --device",
		"android:eas:dev": "eas build --platform android --profile development",
		"android:eas:release": "eas build --platform android --profile production",
		"ios": "expo run:ios",
		"ios:release": "expo run:ios --configuration Release",
		"ios:eas:dev": "eas build --platform ios --profile development",
		"ios:eas:release": "eas build --platform ios --profile production",
		"ios:device": "expo run:ios --device",
		"ios:device:release": "expo run:ios --device --configuration Release",
		"lint": "eslint --report-unused-disable-directives --max-warnings 0 --no-warn-ignored",
		"postinstall": "patch-package",
		"prebuild": "expo prebuild",
		"prebuild:clean": "npm run clean && expo prebuild --clean",
		"prebuild:ci:android": "npm run superclean && expo prebuild --clean --platform android",
		"prebuild:ci:ios": "npm run superclean && expo prebuild --clean --platform ios",
		"typecheck": "tsc -p ./tsconfig.json --noEmit",
		"ncu": "ncu --interactive --format group",
		"clean": "tsx ./clean.ts",
		"superclean": "tsx ./superclean.ts"
	},
	"dependencies": {
		"@expo/vector-icons": "^15.0.3",
		"@react-navigation/bottom-tabs": "^7.4.0",
		"@react-navigation/elements": "^2.7.0",
		"@react-navigation/native": "^7.1.8",
		"expo": "~54.0.19",
		"expo-constants": "~18.0.10",
		"expo-font": "~14.0.9",
		"expo-haptics": "~15.0.7",
		"expo-image": "~3.0.10",
		"expo-linking": "~8.0.8",
		"expo-router": "~6.0.13",
		"expo-splash-screen": "~31.0.10",
		"expo-status-bar": "~3.0.8",
		"expo-symbols": "~1.0.7",
		"expo-system-ui": "~6.0.8",
		"expo-web-browser": "~15.0.8",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.5",
		"react-native-boost": "^0.6.2",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-reanimated": "~4.1.3",
		"react-native-safe-area-context": "~5.6.1",
		"react-native-screens": "~4.16.0",
		"react-native-web": "~0.21.2",
		"react-native-worklets": "0.5.1",
		"tailwindcss": "^4.1.16",
		"uniwind": "^1.0.0-rc.4"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^5.91.2",
		"@types/mime": "^3.0.4",
		"@types/react": "~19.1.10",
		"@types/uuid": "^10.0.0",
		"eslint": "^9.38.0",
		"eslint-config-expo": "~10.0.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-compiler": "^19.1.0-rc.2",
		"eslint-plugin-react-hooks": "^7.0.0",
		"npm-check-updates": "^19.1.1",
		"patch-package": "^8.0.1",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.2"
	}
}
