{
	"name": "@filen/utils",
	"version": "0.0.1",
	"description": "A collection of utils for Filen",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/FilenCloudDienste/filen-ts/packages/filen-utils"
	},
